{
  "name": "10pdf-render-server",
  "version": "3.0.0",
  "description": "a pdf render server",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./server/index.js",
    "dev:site": "webpack serve -c ./dev/build/webpack.dev.js",
    "dev:server": "nodemon server",
    "build": "webpack -c ./dev/build/webpack.prod.js",
    "commit": "git-cz",
    "prepare": "husky install",
    "release": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hezhengxu2018/10pdf-render-server.git"
  },
  "keywords": [
    "pdf.js"
  ],
  "author": "hezhengxu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hezhengxu2018/10pdf-render-server/issues"
  },
  "homepage": "https://github.com/hezhengxu2018/10pdf-render-server#readme",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.1",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-prettier-vue": "^3.0.0",
    "eslint-plugin-vue": "^7.9.0",
    "file-saver": "^2.0.5",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "release-it": "^14.9.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.7",
    "vue-observe-visibility": "^1.0.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "canvas": "^2.7.0",
    "koa": "^2.13.1",
    "koa-helmet": "^6.1.0",
    "koa-json-error": "^3.1.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "lodash.range": "^3.2.0",
    "md5": "^2.3.0",
    "mongoose": "^5.13.1",
    "opn": "^5.5.0",
    "pdfjs-dist": "^2.7.570",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
